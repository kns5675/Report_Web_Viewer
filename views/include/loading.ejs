<!-- 이미지, HTML, PDF 내보내기에 필요한 로딩 창 -->
<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="utf-8">
    <script src="js/modal.js"></script>
    <style>
        #loadingDown{
            float:left; margin:0px auto;
            width:100%; height:90%;
            background-color:#D7DFE7; position:relative;
        }
        #setMiddle{
            background-color:#D7DFE7; height:90px;
            width:275px; float:left;position:relative; top:30px; left:18px;
        }
        #textPart{
            background-color:#D7DFE7; width:90%;height:50%;
            position:absolute;left:5%;margin:0px auto;
        }
        #loadingBar{
            background-color:#D7DFE7; width:90%;height:50%;
            position:absolute;left:5%;top:50%;float:left;
        }
        #imageType{
            font-weight: bold;
        }
        #textWrap{
            position:absolute; margin-left:40px; margin-top:10px;
            background-color:#D7DFE7; width:150px;font-size:14px;
            text-align:Center;
        }
    </style>
</head>
<body>
<div class="modal-content" style="width:310px; height:170px; overflow: visible;">
    <div class="upperbar" style="width:310px;">
        <span style="width:50%; text-align:left; float:left;margin-left:10px;">Duzon Report Viewer</span>
        <!--<span id="closebtn"class="pop_bt" style="cursor:pointer;float:right;margin-right:10px;">X</span>-->
    </div>
    <div id="loadingDown">
            <div id="setMiddle">
                <div id="textPart">
                        <span id="textWrap">
                            <span id="imageType"></span>
                            변환중 입니다.</span><br/>
                </div>
                <div id="loadingBar">
                    <img src="image/loadingbar.gif" alt="loading bar" style="margin-left:17%;margin-top:5%; float:left;">
                </div>

            </div>
            <div class="close" style="float:left; background-color:#D7DFE7;position:absolute; top:110px; left:120px;">
                <!--<input type="button" name="close" onclick="" value="취소" tabindex="3" style="cursor:pointer;width:70px;height:25px;">-->
            </div>
    </div>
</div>
</body>
</html>
